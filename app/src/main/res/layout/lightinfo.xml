<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <TextView
            android:text="TextView"
            android:layout_width="92dp"
            android:layout_height="28dp" android:id="@+id/idView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="68dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="92dp"
            android:layout_height="28dp" android:id="@+id/levelView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/idView"/>
    <TextView
            android:text="TextView"
            android:layout_width="92dp"
            android:layout_height="28dp" android:id="@+id/statusView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/levelView"/>
    <TextView
            android:text="TextView"
            android:layout_width="92dp"
            android:layout_height="28dp" android:id="@+id/roomIdView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/statusView"/>
</android.support.constraint.ConstraintLayout>